{
  "env": {
    "commonjs": true,
    "es2021": true
  },
  "extends": ["standard", "eslint-config-prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": [
    "eslint-plugin-prettier"
  ],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "prettier/prettier": "error",
    "object-shorthand": ["warn", "never"],
    "space-before-function-paren": ["error", { "anonymous": "always", "named": "never", "asyncArrow": "always" }]
  }
}
